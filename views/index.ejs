<body id="body" class="bg-gray-900 transition-all duration-300 ease-in-out">

    <!------ POP UP MESSAGE ------>

    <% if(show) { %>
    <div class="body-layer show-body-layer z-50 opacity-75 fixed bg-gray-800 w-screen h-screen">
    </div>
    <div class="pop-up show-pop-up fixed z-50 flex flex-col w-4/12 h-3">
      <div class="modal p-6 rounded-xl opacity-100 bg-white">
        <h1 id="modal-title" class="text-xl z-50 my-2 font-semibold text-black">
            <%= title %>
        </h1>
        <p id="modal-text" class="leading my-2">
            <%= text %>
        </p>
        <button id="modal-btn" type="button" class="bg-indigo-600 text-white py-2 px-6 font-semibold mt-2 rounded-md hover:bg-indigo-500 focus:shadow-outline focus:outline-none cursor-pointer ease-in-out duration-300">Okay</button>
      </div>
    </div>
    <% } else { %>
      <div class="body-layer z-50 opacity-75 fixed bg-gray-800 w-screen h-screen">
      </div>
      <div class="pop-up fixed z-50 flex flex-col w-4/12 h-3">
        <div class="modal p-6 rounded-xl opacity-100 bg-white">
          <h1 id="modal-title" class="text-xl z-50 my-2 font-semibold text-black">
          </h1>
          <p id="modal-text" class="leading my-2">
          </p>
          <button id="modal-btn" type="button" class="bg-indigo-600 text-white py-2 px-6 font-semibold mt-2 rounded-md hover:bg-indigo-500 focus:shadow-outline focus:outline-none cursor-pointer ease-in-out duration-300">Okay</button>
        </div>
      </div>
    <% } %>
    
